    "version": "0.2.0",
    "configurations": [
        {
            "name": "lunar_pitch",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "lunar_pitch (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "lunar_pitch (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
        },
        {
            "name": "Flutter Web",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "args": [
                "--web-port=5000",
                "--web-enable-expression-evaluation"
            ],
            "deviceId": "chrome"
        },
        {
            "name": "Flutter Web (No Debugging)",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "deviceId": "chrome"
        },
        {
            "name": "Flutter Web (Release)",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "args": [
                "--release",
                "--web-port=5000"
            ],
            "deviceId": "chrome"
        }
    ]
}